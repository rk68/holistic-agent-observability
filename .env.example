# Agent settings
AGENT_MODEL=qwen3:4b
AGENT_NAME=glass-react-agent
AGENT_TEMPERATURE=0.2
AGENT_ENABLE_TRACING=false
AGENT_ENABLE_RESOURCE_TOOL=true
AGENT_ENABLE_MATH_TOOLS=true

# Backend selection: ollama | aws | aws_gateway
AGENT_BACKEND=ollama

# Ollama
OLLAMA_BASE_URL=http://localhost:11434

# Langfuse (optional, for remote traces)
LANGFUSE_HOST=
LANGFUSE_BASE_URL=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=

# AWS Bedrock-style gateway (optional)
AWS_BEDROCK_API_ENDPOINT=
AWS_BEDROCK_API_KEY=
AWS_BEDROCK_TEAM_ID=
AWS_AGENT_MODEL=
